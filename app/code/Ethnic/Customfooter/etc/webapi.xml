<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../app/code/Magento/Webapi/etc/webapi.xsd">
    <route method="GET" url="/V1/ethnic/customfooter-api/">
        <service class="Ethnic\Customfooter\Api\AllnewsRepositoryInterface" method="getPost"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="GET" url="/V1/ethnic/customfooter/getdata/">
        <service class="Ethnic\Customfooter\Api\AllnewsRepositoryInterface" method="getData"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="GET" url="/V1/ethnic/customfooter/delete/">
        <service class="Ethnic\Customfooter\Api\AllnewsRepositoryInterface" method="getDelete"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="GET" url="/V1/ethnic/customfooter/getbyid/">
        <service class="Ethnic\Customfooter\Api\AllnewsRepositoryInterface" method="getById"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="GET" url="/V1/ethnic/customfooter/edit/">
        <service class="Ethnic\Customfooter\Api\AllnewsRepositoryInterface" method="getEdit"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>