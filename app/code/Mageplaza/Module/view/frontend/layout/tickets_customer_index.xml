<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
	<update handle="customer_account"/>
	<body>
		<referenceBlock name="page.main.title">
			<action method="setPageTitle">
				<argument translate="true" name="title" xsi:type="string">My Tickets</argument>
			</action>
		</referenceBlock>
		<referenceContainer name="content">
			<!--<block class="Magento\Framework\View\Element\Template" name="my_tab" template="Mageplaza_Module::mytab.phtml" cacheable="false" />-->
			<block class="Dynamic\Mytickets\Block\Myticketslist" name="my_tab" template="Dynamic_Mytickets::mytickets-list.phtml" cacheable="false" />
		</referenceContainer>
	</body>
</page>
