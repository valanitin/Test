<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_form_login">
            <action ifconfig="psloginfree/general/replace_templates" method="setTemplate">
                <argument name="template" xsi:type="string">Plumrocket_SocialLoginFree::customer/form/login.phtml</argument>
            </action>
            <block class="Plumrocket\SocialLoginFree\Block\Buttons" name="pslogin.customer.form.login.buttons" as="pslogin_buttons" template="Plumrocket_SocialLoginFree::customer/form/login/buttons.phtml" />
        </referenceBlock>
    </body>
</page>
