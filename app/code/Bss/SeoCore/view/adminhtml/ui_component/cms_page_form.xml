<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="search_engine_optimisation">
        <field name="seo_h1_title">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="dataType" xsi:type="string">string</item>
                    <item name="formElement" xsi:type="string">input</item>
                    <item name="label" xsi:type="string" translate="true">Custom H1 Page</item>
                    <item name="sortOrder" xsi:type="number">132</item>
                    <item name="notice" xsi:type="string" translate="true">This new heading 1 will replace the original h1 of your page</item>
                </item>
            </argument>
        </field>
    </fieldset>
</form>